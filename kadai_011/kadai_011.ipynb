      "source": [
        "array = [\"水\", \"金\", \"地\", \"火\", \"木\", \"土\", \"天\", \"海\", \"冥\"]\n",
        "\n",
        "# for文でリストの要素を取り出す\n",
        "print(\"【for文による出力】\")\n",
        "for element in array:\n",
        "    print(element)\n",
        "\n",
        "# while文でリストの要素を取り出す\n",
        "print(\"【while文による出力】\")\n",
        "i = 0\n",
        "while i < len(array):\n",
        "    print(array[i])\n",
        "    i += 1"
      ]
    }
